{
  "name": "at-grid",
  "version": "1.0.6",
  "description": "AtGrid",
  "main": "at-grid.umd.js",
  "jsnext:main": "at-grid.esm.js",
  "module": "at-grid.esm.js",
  "types": "at-grid-source.d.ts",
  "typings": "at-grid-source.d.ts",
  "scripts": {
    "copy": "node tools/copy-package.js && node tools/cleanup-package.js",
    "bundle": "rimraf dist && rollup -c rollup.config.umd.js && rollup -c rollup.config.esm.js && ngc",
    "postbundle": "node tools/removecode.js",
    "build": "npm run bundle && npm run copy && npm run postbundle",
    "tsc-es": "tsc -p tsconfig-es.json",
    "ngc-es": "ngc -p tsconfig-es.json",
    "tsc": "tsc",
    "ngc": "ngc",
    "rollup-umd": "rollup -c rollup.config.umd.js",
    "rollup-es": "rollup -c rollup.config.esm.js",
    "publish": "cd ./dist && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolay-rogovoy/at-grid.git"
  },
  "keywords": [
    "datagrid"
  ],
  "author": "n_g <nikolay.rogovoy@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "^4.2.6",
    "@angular/compiler": "^4.2.6",
    "@angular/forms": "^4.2.6",
    "@angular/core": "^4.2.6",
    "@angular/platform-browser": "^4.2.6",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.13"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.2.6",
    "@types/node": "^8.0.11",
    "del": "^2.2.2",
    "rimraf": "^2.5.4",
    "node-sass": "^4.5.3",
    "rollup": "^0.45.2",
    "rollup-plugin-angular": "^0.4.4",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^2.0.1",
    "typescript": "~2.3.3"
  },
  "bugs": {
    "url": "https://github.com/nikolay-rogovoy/at-grid/issues"
  },
  "homepage": "https://github.com/nikolay-rogovoy/at-grid#readme"
}
